      Problem from
       
       
      Suppose we have a weighted complete graph with n vertices. We want to colour
      each vertex one of k colours. The cost of any colouring is the sum of the
      weights of all edges that connect vertices of the same colour.
       
      For my use case, n will be around 20-50. For k = 2, 3 brute force worked
      fine, but I would ideally like a solution for any k. It is also the case
      that the graphs I'm dealing with have reasonable sparsity, but again,
      I would ideally like to solve the general case.
       
  15   
  16   
  17  *---------------------------------------------------------------
  18  * random graph
  19  *
  20  * sparse undirected network:
  21  * arcs only from i->j when i<j
  22  *---------------------------------------------------------------
  23   
  24  set
  25     n 'nodes' /n1*n10/
  26     a(n,n) 'arcs'
  27     c 'colors' /orange,lightblue,brown/
  28  ;
  29   
  30  alias(n,i,j);
  31   
  32  a(i,j)$(ord(i)<ord(j)) = yes;
  33   
  34  parameter w(n,n) 'weights';
  35  w(a) = uniform(0,1);
  36  display w;
  37   
  38   
  39  *---------------------------------------------------------------
  40  * MIQP model
  41  *---------------------------------------------------------------
  42   
  43  binary variables
  44     x(n,c) 'assign color to node'
  45  ;
  46   
  47  variable
  48     cost 'number of arcs with same colored nodes'
  49  ;
  50   
  51  equations
  52     objective            'minimize cost'
  53     assign(n)            'every node should have exactly one color'
  54  ;
  55   
  56  objective..  cost =e= sum((a(i,j),c),w(a)*x(i,c)*x(j,c));
  57   
  58  assign(n)..  sum(c, x(n,c)) =e= 1;
  59   
  60  model color1 /all/;
  61  option optcr=0, miqcp=cplex, threads=16;
  62  solve color1 minimizing cost using miqcp;
  63   
  64   
  65  *---------------------------------------------------------------
  66  * MIP model
  67  *---------------------------------------------------------------
  68   
  69  binary variable y(i,j) 'nodes have the same color';
  70   
  71  equations
  72     objective2    'linearized objective'
  73     ybound(i,j,c) 'x(i,c)=x(j,c)=1 => y(i,j)=1'
  74  ;
  75   
  76  objective2..        cost =e= sum(a,w(a)*y(a));
  77  ybound(a(i,j),c)..  y(i,j) =g= x(i,c)+x(j,c)-1;
  78   
  79  model color2 /objective2,assign,ybound/;
  80   
  81  * optionally relax y to continuous between 0 and 1
  82  y.prior(a) = INF;
  83   
  84  solve color2 minimizing cost using mip;
  85   
  86   
  87  *---------------------------------------------------------------
  88  * reporting
  89  *---------------------------------------------------------------
  90   
  91  display x.l,cost.l;
  92   
  93   
  94  parameter wx 'obj contribution';
  95  wx(a(i,j)) = w(a)*sum(c,x.l(i,c)*x.l(j,c));
  96  display wx;
  97   
  98   
  99   
 100  *-----------------------------------------------------------
 101  * visualization using GraphViz
 102  *-----------------------------------------------------------
 103   
 104   
 105  file gv2 /graph2.gv/;
 106  put gv2;
 107  put "graph neato{"/;
 108  loop((n,c)$(x.l(n,c)>0.5),
 109     put "node [shape=circle,style=filled,color=",c.tl:0,"] ",n.tl:0/;
 110  );
 111   
 112   
 113  parameter ncol(n);
 114  ncol(n) = sum(c, ord(c)*round(x.l(n,c)));
 115   
 116  loop(a(i,j),
 117     if (ncol(i)=ncol(j),
 118         loop(c$(x.l(i,c)>0.5),
 119         put "edge [color=",c.tl:0,",penwidth=4] ",i.tl:0,"--",j.tl:0/;
 120         );
 121     else
 122        put "edge [color=grey,penwidth=1] ",i.tl:0,"--",j.tl:0/;
 123     )
 124  );
 125   
 126  putclose"}"/;
