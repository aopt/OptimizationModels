C o m p i l a t i o n


       
         Here we consider a directed network. We want to find all feasible
         paths from A->B such that arcs are used only once (nodes can be
         visited several times).
       
         We use an augmented graph for this, in combination with the
         MTZ subtour-elimination constraint (from TSP models). We solve
         using the solution pool.
       
         References:
           https://yetanothermathprogrammingconsultant.blogspot.com/2022/01/an-all-paths-network-problem.html
       
  15   
  16  *-----------------------------------------------------------
  17  * network topology
  18  *-----------------------------------------------------------
  19   
  20  option seed=1;
  21   
  22  set
  23     n 'nodes' /n1*n10/
  24     a(n,n) 'directed arcs'
  25  ;
  26   
  27  alias (n,i,j);
  28   
  29  a(i,j) = uniform(0,1)<0.2;
  30   
  31  scalar M 'maximum hops' /3/;
  32   
  33  display n,a,M;
  34   
  35   
  36  singleton sets
  37     src(n) 'source' /n1/
  38     snk(n) 'sink'   /n10/
  39  ;
  40  set ni(n) 'interior nodes';
  41  ni(n) = yes;
  42  ni(src) = no;
  43  ni(snk) = no;
  44   
  45   
  46  *-----------------------------------------------------------
  47  * visualization using GraphViz
  48  *-----------------------------------------------------------
  49   
  50  file gv1 /graph1.gv/;
  51  put gv1;
  52  put "digraph neato{"/;
  53  put "node [shape=circle,style=filled,color=yellowgreen] ",src.tl/;
  54  put "node [shape=circle,style=filled,color=orange] ",snk.tl/;
  55  put "node [shape=circle,style=filled,color=lightblue]"/;
  56  loop(ni, put ni.tl:0," ";); put /;
  57  put "edge [color=grey]"/;
  58  loop(a(i,j), put i.tl:0,"->",j.tl:0/;);
  59  putclose"}"/;
  60   
  61  *-----------------------------------------------------------
  62  *   shortest path n1->n2 as MIP/RMIP model
  63  *-----------------------------------------------------------
  64   
  65  parameter inflow(n) 'exogenous inflow';
  66  inflow(src) = 1;
  67  inflow(snk) = -1;
  68   
  69  binary variable f(i,j) 'flow';
  70  variable count 'length of shortest path (hops)';
  71   
  72  equations
  73      obj           'objective'
  74      flowbal(n)    'flow balance at node'
  75  ;
  76   
  77  obj.. count =e= sum(a,f(a));
  78  flowbal(n)..  sum(a(i,n),f(a)) + inflow(n) =e= sum(a(n,j),f(a));
  79   
  80  model sp /all/;
  81  solve sp minimizing count using rmip;
  82   
  83  display "#### Model SP: Single Shortest Path Solution", count.l,f.l;
  84   
  85   
  86  *-----------------------------------------------------------
  87  *   add hop count
  88  *   and solve with solution pool
  89  *-----------------------------------------------------------
  90   
  91  count.up = M;
  92   
  93  sp.optfile=1;
  94  solve sp minimizing count using mip;
  95   
  96  set
  97    solIndex /soln_sp_p1*soln_sp_p20
  98              soln_sp2_p1*soln_sp2_p20
  99              soln_sp3_p1*soln_sp3_p20/
 100    sol(solIndex)
 101  ;
 102  parameter fsol(solIndex,n,n) 'solution from solution pool';
 103   
 104  execute_load "solutions.gdx",sol=index,fsol=f;
 105  option fsol:0:1:1;
 106  display "#### Model SP/solution pool: Find all feasible paths with max length",
 107           count.up,sol,fsol,
 108          "#### WARNING: This solution contains subtours";
 109   
 110  *-----------------------------------------------------------
 111  *   add MTZ subtour elimination constraints
 112  *-----------------------------------------------------------
 113   
 114  positive variable t(i) 'ordering of hops';
 115   
 116  equation mtz(i,j) 'subtour elimination constraints';
 117   
 118  mtz(a(i,j))$(not src(i) and not snk(j))..
 119      t(j) =g= t(i) + 1 + card(n)*(f(i,j)-1);
 120   
 121  model sp2 /sp+mtz/;
 122  sp2.optfile=1;
 123  solve sp2 minimizing count using mip;
 124   
 125  execute_load "solutions.gdx",sol=index,fsol=f;
 126  display "#### Model SP2/solution pool: Find all feasible paths with max length",
 127           sol,fsol,
 128          "#### WARNING: This solution excludes tours";
 129   
 130   
 131  *-----------------------------------------------------------
 132  *  make arcs nodes, and arcs indicate adjacency
 133  *-----------------------------------------------------------
 134   
 135  set in(*)  'references to old nodes while adding src/snk' /src,snk/;
 136  in(n) = yes;
 137  alias(in,jn,kn);
 138   
 139  set nn(*,*) 'new nodes' /src.n1, n10.snk/;
 140  nn(a) = yes;
 141  display nn;
 142  alias (nn,nn2);
 143   
 144  set an(*,*,*,*) 'new arcs (w/o self-loops)';
 145  an(in,jn,jn,kn)$(nn(in,jn) and nn(jn,kn)
 146                  and not (sameas(in,jn) and sameas(jn,kn))) = yes;
 147  option an:0:2:2;
 148  display an;
 149   
 150   
 151  *-----------------------------------------------------------
 152  * visualization using GraphViz
 153  *-----------------------------------------------------------
 154   
 155  file gv2 /graph2.gv/;
 156  put gv2;
 157  put "digraph neato{"/;
 158  put "node [shape=circle,style=filled,color=yellowgreen] ";
 159  loop(nn('src',in), put 'src_':0,in.tl:0/ );
 160  put "node [shape=circle,style=filled,color=orange] ";
 161  loop(nn(in,'snk'), put in.tl:0,'_snk':0/ );
 162  put "node [shape=circle,style=filled,color=lightblue]"/;
 163  loop(nn(in,jn)$(not sameas(in,'src') and not sameas(jn,'snk')),
 164      put in.tl:0,"_":0,jn.tl:0," ":0;);
 165  put /;
 166  put "edge [color=grey]"/;
 167  loop(an(in,jn,jn,kn),
 168     put in.tl:0,"_":0,jn.tl:0,"->":0,jn.tl:0,"_",kn.tl:0/);
 169  putclose"}"/;
 170   
 171  *-----------------------------------------------------------
 172  * Model v2
 173  *-----------------------------------------------------------
 174   
 175  M = 4;
 176   
 177  singleton sets
 178     srcn(*,*) / src.n1 /
 179     snkn(*,*) /n10.snk /
 180  ;
 181   
 182  parameter inflown(*,*) 'new inflow';
 183  inflown(srcn) = 1;
 184  inflown(snkn) = -1;
 185   
 186  binary variables fn 'new flow variables';
 187  positive variable tn 'MTZ variable';
 188   
 189  equations
 190      objn     'new objective'
 191      flowbaln 'new flow balance on nodes'
 192      mtzn     'new MTZ subtour elimination constraints'
 193  ;
 194   
 195  objn.. count =e= sum(an,fn(an));
 196  flowbaln(nn).. sum(an(nn2,nn),fn(an)) + inflown(nn) =e= sum(an(nn,nn2),fn(an));
 197  mtzn(an(nn,nn2))$(not srcn(nn) and not snkn(nn2))..
 198      tn(nn2) =g= tn(nn) + 1 + card(nn)*(fn(nn,nn2)-1);
 199  count.up = M+1;
 200   
 201  model sp3 /objn,flowbaln,mtzn/;
 202  sp3.optfile=1;
 203  solve sp3 minimizing count using mip;
 204   
 205  parameter fsoln(solIndex,*,*,*,*) 'solution from solution pool';
 206   
 207  execute_load "solutions.gdx",sol=index,fsoln=fn;
 208  option fsoln:0:2:2;
 209  display "#### Model SP3/solution pool: augmented graph",
 210           count.up,sol,fsoln;
