  26  parameter WIOT(*,*,*,*) 'WIOT Intercountry Input-Output Table';
GDXIN   D:\projects\OptimizationModels\LinSolve\WIOT2014_Nov16_ROW.gdx
--- LOAD  WIOT = 1:wiot
  29   
  30   
  31  *----------------------------------------------------------------
  32  * Sets
  33  *----------------------------------------------------------------
  34   
  35  sets
  36     i 'industries' /
  37        A01,A02,A03,B,C10-C12,C13-C15,C16,C17,C18,C19,C20,C21
  38        C22,C23,C24,C25,C26,C27,C28,C29,C30,C31_C32,C33,D35,E36
  39        E37-E39,F,G45,G46,G47,H49,H50,H51,H52,H53,I,J58,J59_J60
  40        J61,J62_J63,K64,K65,K66,L68,M69_M70,M71,M72,M73,M74_M75
  41        N,O84,P85,Q,R_S,T,U
  42      /
  43     r 'regions (43 countries + ROW)' /
  44        AUS,AUT,BEL,BGR,BRA,CAN,CHE,CHN,CYP,CZE,DEU,DNK,ESP
  45        EST,FIN,FRA,GBR,GRC,HRV,HUN,IDN,IND,IRL,ITA,JPN,KOR
  46        LTU,LUX,LVA,MEX,MLT,NLD,NOR,POL,PRT,ROU,RUS,SVK,SVN
  47        SWE,TUR,TWN,USA,ROW
  48      /
  49    ir(i,r)
  50  ;
  51  ir(i,r) = yes;
  52  alias(i,ii,i2,i3),(r,rr,r2,r3),(ir,ir2,ir3);
  53   
  54   
  55  *----------------------------------------------------------------
  56  * do some counting
  57  *----------------------------------------------------------------
  58   
  59  parameter counts(*) 'industries,regions,combinations';
  60  counts('i:sectors') = card(i);
  61  counts('r:regions') = card(r);
  62  counts('ir:combinations') = card(ir);
  63  counts('density') = sum((ir,ir2)$WIOT(ir,ir2),1)/sqr(card(ir));
  64  option counts:0;
  65  display counts;
  66   
  67  *----------------------------------------------------------------
  68  * extract parameters
  69  *----------------------------------------------------------------
  70   
  71  set
  72    fd 'final demand'/
  73            CONS_h  'Final consumption expenditure by households'
  74            CONS_np 'Final consumption expenditure by non-profit organisations serving households (NPISH)'
  75            CONS_g  'Final consumption expenditure by government'
  76            GFCF    'Gross fixed capital formation'
  77            INVEN   'Changes in inventories and valuables'
  78         /
  79  ;
  80   
  81  parameter
  82     finalDemand(i,r) 'final demand f'
  83     grossOutput(i,r) 'total'
  84     A(i,r,i,r)       'technological coefficients'
  85     Ident(i,r,i,r)   'Indentity matrix I'
  86  ;
  87  finalDemand(ir) = sum((fd,r),WIOT(ir,fd,r));
  88  grossOutput(ir) = WIOT(ir,'GO','TOT');
  89  A(ir,ir2)$grossOutput(ir2) = WIOT(ir,ir2)/grossOutput(ir2);
  90  Ident(i,r,i,r) = 1;
  91   
  92  *----------------------------------------------------------------
  93  * sanity check
  94  *----------------------------------------------------------------
  95   
  96  * x = Z*1 + f
  97   
  98  parameter Xerr(i,r) 'error in identity x = Z*1 + f';
  99  Xerr(ir) = grossOutput(ir) - sum(ir2,WIOT(ir,ir2)) - finalDemand(ir);
 100  abort$(smax(ir,abs(Xerr(ir)))>1e-5) "Check Xerr";
 101   
