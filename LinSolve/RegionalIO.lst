 104  *----------------------------------------------------------------
 105   
 106  parameter L(i,r,i,r) 'Leontief inverse';
 107   
BATINCLUDE D:\projects\OptimizationModels\LinSolve\leontief.inc
       
       leontief.inc
       
       Calculate the leontief inverse.
       
       Arguments:
         %1 : input matrix: square matrix with technological coefficients
         %2 : output matrix: Leontief matrix
         %3 : sets defining A and L
              can have 1 or multiple dimensions
       
 122   
 232   
 233  *----------------------------------------------------------------
 234  * some checks
 235  *----------------------------------------------------------------
 236   
 237  * check  (I-A) * inv(I-A) = I
 238  * do a small subset of countries (a complete check would take a long time).
 239  set rsub(r) /AUS,AUT/;
 240  parameter InvErr(i,r,i,r) 'error in identity (I-A)*L=I';
 241  InvErr(ir(i,r),ir2(i2,r2))$(rsub(r) and rsub(r2)) =
 242      sum(ir3, (Ident(ir,ir3)-A(ir,ir3))*L(ir3,ir2)) - Ident(ir,ir2);
 243  abort$(smax((ir,ir2),abs(InvErr(ir,ir2)))>1e-5) "Check InvErr";
 244   
 245   
 246  * check  x = inv(I-A)*f = L*f
 247  parameter xInvErr(i,r) 'error in x = inv(I-A)*f';
 248  xInvErr(ir) = sum(ir2, L(ir,ir2)*finalDemand(ir2)) - GrossOutput(ir);
 249  abort$(smax(ir,abs(XInvErr(ir)))>1e-5) "Check XInvErr";
 250   
STOP D:\projects\OptimizationModels\LinSolve\RegionalIO.gms
